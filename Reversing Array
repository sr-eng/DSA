using namespace std;

int main()
{
    int n,i,A[n];
    
    cout<<"Enter no of elements in an array: ";
    cin>>n;
    cout<<"Array is : ";
    for(i=0;i<n;i++){
        cin>>A[i];
    }
    int rev[n],j=0;
    for(i=n-1;i>=0;i--){
        rev[j]=A[i];
        j++;
    }
    cout<<"Reversed Array is: ";
    for(i=0;i<n;i++){
        cout<<rev[i];
    }
}
